{
  "extends": [
    "airbnb",
    "prettier"
  ],
  "parser": "typescript",
  "plugins": [
    "import",
    "jest",
    "prettier"
  ],
  "env": {
    "jest": true,
    "es6": true
  },
   "rules": {
      "no-undef": 0,
      "prettier/prettier": [2, {
        "trailingComma": "all",
        "singleQuote": true,
        "parser": "typescript",
      }]
   }
}
