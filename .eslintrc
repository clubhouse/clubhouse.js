{
  "extends": [
    "airbnb",
    "prettier",
    "prettier/@typescript-eslint",
    "plugin:@typescript-eslint/recommended"
  ],
  "parser":  "@typescript-eslint/parser",
  "parserOptions": {
    "sourceType": "module"
  },
  "settings": {
    "import/extensions": [".js", ".ts"],
    "import/parsers": {
      "@typescript-eslint/parser": [".ts"]
    },
    "import/resolver": {
      "node": {
        "extensions": [".js", ".ts"]
      }
    }
  },
  "plugins": [
    "import",
    "jest",
    "prettier"
  ],
  "env": {
    "jest": true,
    "es6": true
  },
   "rules": {
      "no-undef": 0,
      "@typescript-eslint/camelcase": 0
   }
}
